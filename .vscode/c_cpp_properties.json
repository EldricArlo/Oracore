{
    "configurations": [
        {
            "name": "Win-UCRT64-GCC",
            "includePath": [
                "${workspaceFolder}/**",
                "D:/MSYS2/ucrt64/include/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                // 添加这个宏来防止 wincrypt.h 的宏定义冲突
                "NOCRYPT"
            ],
            "compilerPath": "D:/MSYS2/ucrt64/bin/gcc.exe",
            "cStandard": "c11", // 您的项目使用c11标准
            "cppStandard": "gnu++17",
            "intelliSenseMode": "windows-gcc-x64"
            // 删除了 "configurationProvider" 这一行
        }
    ],
    "version": 4
}